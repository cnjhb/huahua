<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="item" type="asia.jhb.huahua.VoltageItem"/>
    </data>
    <androidx.cardview.widget.CardView android:layout_width="match_parent" android:layout_height="wrap_content"     android:layout_margin="2dp"
                                       app:cardCornerRadius="4dp"
                   android:id="@+id/container_voltage">
        <LinearLayout android:layout_width="match_parent" android:layout_height="match_parent" android:orientation="vertical">
        <TextView android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@{String.valueOf(item.frequency)}"/>
        <TextView android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@{item.voltage.name()}"/>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</layout>